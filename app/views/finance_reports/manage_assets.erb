<div class="main">
  <div class="box">
    <div class="data_box">
      <div class="tab_head">
        <ul>
          <li class="hover">资产列表</li>
        </ul>
      </div>
      <div class="data_body">
        <div>
          <div class="search">
            <div><label>时间：</label>
              <input id="c_first" type="text"  onclick="WdatePicker({preLoad:true,maxDate:'<%=Time.now.strftime("%Y-%m-%d")%>'})" readonly class="Wdate"
                     value="<%=@start_time.strftime("%Y-%m-%d")%>"/> 到
              <input id="c_last" type="text" onclick="WdatePicker({preLoad:true,maxDate:'<%=Time.now.strftime("%Y-%m-%d")%>'})" readonly class="Wdate"
                     value="<%=@end_time.strftime("%Y-%m-%d")%>" /></div>
            <div style="float:right;"><button class="search_btn" style="padding:-1px 0;" onclick="manage_assets(this,<%=params[:store_id]%>)">查&nbsp&nbsp询</button>
            <button class="search_btn" onclick="show_center('#add_tab')">新 &nbsp&nbsp&nbsp&nbsp建</button></div>
          </div>
          <div class="data_item" id="manage_assets">
            <%=  render "manage_assets"%>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<div class="tab_popup add_tab" id="add_tab">
  <div class="popup_box">
    <h1>新建资产<a href="javascript:void(0)" class="close">关闭</a></h1>
    <div class="popup_body">
      <%= form_for :new_assets,:html=>{:id=>:new_assets},:method=>:post,:url=>"/stores/#{params[:store_id]}/finance_reports/create_assets" do %>
        <div class="popup_body_area">
          <%= render "assets_form" %>
        </div>
        <div class="btn_box_s"><input value="确定" class="confirm_btn" onclick="check_assets(this)" type="button"  /></div>
      <%end%>
    </div>
  </div>
</div>
<div class="tab_popup show_asset" id="show_asset"></div>
