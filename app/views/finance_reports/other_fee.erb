<div class="main">
  <div class="box">
    <div class="data_box">
      <div class="tab_head">
        <ul>
          <li onclick="window.location.href='/stores/<%=params[:store_id]%>/finance_reports'">主营收入</li>
          <li class="hover">其他收入</li>
           <li onclick="window.location.href='/stores/<%=params[:store_id]%>/finance_reports/return_order'">退单记录</li>
          <li onclick="window.location.href='/stores/<%=params[:store_id]%>/finance_reports/revenue_report'">收入报表</li>
        </ul>
      </div>
      <div class="data_body">
        <div>
          <div class="search">
            <div><label>时间：</label><input id="c_first" type="text"  onclick="WdatePicker({preLoad:true,maxDate:'<%=Time.now.strftime("%Y-%m-%d")%>'})" readonly class="Wdate"
                                          value="<%=@start_time%>"/> 到
              <input id="c_last" type="text" onclick="WdatePicker({preLoad:true,maxDate:'<%=Time.now.strftime("%Y-%m-%d")%>'})" readonly class="Wdate"
                     value="<%=@end_time%>" /></div>
            <div><label>客户：</label><input type="text" id="customer_n" value="<%=@customer_n%>" /></div>
            <div><label>卡类：</label><select  id="card_types"><option value="">--</option>
                <% OrderPayType::OTHER_TYPES.each do |key,value| %>
                  <option value="<%= key %>" <%if params[:card_type] == "#{key}"%> selected<%end%> ><%= value %></option>
                <% end%>
              </select></div>
            <div><button class="search_btn" onclick="other_fee(this,<%=params[:store_id]%>)">查&nbsp&nbsp询</button></div>
            <div class="data_menu afterClear"><a href="javascript:void(0)" onclick="single_order_print(<%=params[:store_id]%>)">打印</a></div>
          </div>
          <div class="data_item" id="cash_over">
            <%=  render "cash_over"%>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>