<div class="main">
  <div class="box">
    <div class="data_box">
      <div class="tab_head">
        <ul>
          <li onclick="window.location.href='/stores/<%=params[:store_id]%>/finance_reports/fee_manage'" >费用明细</li>
          <li class="hover">费用报表</li>
        </ul>
      </div>
      <div class="data_body">
        <div class="search">
          <div><label>时间：</label>
            <input id="c_first" type="text"  onclick="WdatePicker({preLoad:true,maxDate:'<%=Time.now.strftime("%Y-%m-%d")%>'})" readonly class="Wdate"
                   value="<%=@start_time%>"/> 到
            <input id="c_last" type="text" onclick="WdatePicker({preLoad:true,maxDate:'<%=Time.now.strftime("%Y-%m-%d")%>'})" readonly class="Wdate"
                   value="<%=@end_time%>" /></div>
          <div style="float:right;"><button class="search_btn" style="padding:-1px 0;" onclick="fee_report(this,<%=params[:store_id]%>)">查&nbsp&nbsp询</button></div>
        </div>
        <div class="data_item" id="fee_report">
          <%= render 'fee_report' %>
        </div>
      </div>
    </div>
  </div>
</div>
