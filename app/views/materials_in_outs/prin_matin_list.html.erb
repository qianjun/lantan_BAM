<table width="100%" border="0" cellspacing="0" cellpadding="0" class="data_table">
  <thead>
    <tr class="hbg">
      <td>订货编号</td>
      <td>条形码</td>
      <td>物料名称</td>
      <td>数量</td>
    </tr>
  </thead>
  <tbody class="mat-in-list">
    <% @mat_in_list_hash.each do |mat_in| %>
      <tr>
        <td><%= mat_in.mo_code %></td>
        <td><%= mat_in.mat_code %></td>
        <td><%= mat.mat_name %></td>
        <td><%= mat.num %></td>
      </tr>
    <% end %>
  </tbody>
</table>
